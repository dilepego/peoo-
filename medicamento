class UI:
  @classmethod
  def main(cls):
    op = 50
    while op != 0:
      op = UI.menu()
      if op == 1:
        UI.inserir()
      if op == 2:
        UI.listar()
      if op == 3:
        UI.atualizar()
      if op == 4:
        UI.excluir()
      if op == 5:
        UI.vencidos()

  @classmethod
  def menu(cls):
    print('0 - sair | 1 - inserir | 2 - listar | 3 - atualizar | 4 - excluir | 5 - vencidos')
    return int(input())
  
  
